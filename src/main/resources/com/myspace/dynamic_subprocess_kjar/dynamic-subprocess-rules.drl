package com.myspace.dynamic_subprocess_kjar;

rule "Product X process"
when
    $p: Product (name == "Product X")
then
    System.out.println("Found the product X here!");
    insert(new ProcessExchangeModel("child-a", "a product was received with price: " + $p.getPrice()));
end

query "qProcessExchange"
  $px: ProcessExchangeModel()
end