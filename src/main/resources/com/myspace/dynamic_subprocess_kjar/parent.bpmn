        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("Decide subprocess to execute == BASED ON HTTP REST REQUEST");
Product _product = (Product) kcontext.getVariable("product");
ProcessExchangeModel pem = new ProcessExchangeModel("child-b", "Unable to resolve REST Endpoint");
kcontext.setVariable("processExchange", pem);
try {	
  // create HTTP Client
  org.apache.http.client.HttpClient httpClient = org.apache.http.impl.client.HttpClientBuilder.create().build();
 
  // Create new getRequest with below mentioned URL
  org.apache.http.client.methods.HttpPost postRequest = new org.apache.http.client.methods.HttpPost("http://localhost:8080/kie-server/services/rest/server/containers/instances/dynamic-subprocess-rules-kjar");
  // Add additional header to getRequest which accepts application/xml data
  postRequest.addHeader("accept", "application/json");
  postRequest.addHeader("content-type", "application/json");
  // cGFtQWRtaW46cjNkaDR0MSE=
  // from Unix command: $ echo -n pamAdmin:r3dh4t1! | base64
  postRequest.addHeader("Authorization", "Basic cGFtQWRtaW46cjNkaDR0MSE=");
  
  String jsonRequest = "{ \"lookup\": \"default-ksession\",";  // request body open, and stateless ksession lookup
  jsonRequest = jsonRequest + "\"commands\": ["; // commands array open
  
  jsonRequest = jsonRequest + "{"; // insert product command open
  jsonRequest = jsonRequest + "\"insert\": { \"object\":{"; // commands array open
  jsonRequest = jsonRequest + "\"com.myspace.dynamic_subprocess_rules_kjar.Product\": { ";
  jsonRequest = jsonRequest + "\"name\": \"" + _product.getName() + "\",";
  jsonRequest = jsonRequest + "\"price\": " + _product.getPrice() + "";
  jsonRequest = jsonRequest + "}}, \"out-identifier\": \"product\", \"entry-point\": \"DEFAULT\""; // product object close
  jsonRequest = jsonRequest + "}},"; // insert product command close

  jsonRequest = jsonRequest + "{\"insert\": { \"object\":{";; // insert process exchange command open
  jsonRequest = jsonRequest + "\"com.myspace.dynamic_subprocess_rules_kjar.ProcessExchangeModel\": { ";
  jsonRequest = jsonRequest + "\"subprocess\": \"" + pem.getSubprocess() + "\",";
  jsonRequest = jsonRequest + "\"message\": \"" + pem.getMessage() + "\"";
  jsonRequest = jsonRequest + "}}, \"out-identifier\": \"processExchange\", \"entry-point\": \"DEFAULT\""; // process exchange object close
  jsonRequest = jsonRequest + "}},"; // insert process exchange command close

  jsonRequest = jsonRequest + "{\"fire-all-rules\": {\"max\": -1, \"out-identifier\": \"fired\"}}"; // insert fire all rules command
  
  jsonRequest = jsonRequest + "]"; // commands array close
  jsonRequest = jsonRequest + "}"; // request body close

  System.out.println("REQUEST PAYLOAD: " + jsonRequest);

  postRequest.setEntity(new org.apache.http.entity.StringEntity(jsonRequest));
  // Execute your request and catch response
  org.apache.http.HttpResponse response = httpClient.execute(postRequest);
 
  // Check for HTTP response code: 200 = success
  if (response.getStatusLine().getStatusCode() != 200) {
    throw new RuntimeException("Failed : HTTP error code : " + response.getStatusLine().getStatusCode());
  }

  // Get-Capture Complete application/json body response
  java.io.BufferedReader br = new java.io.BufferedReader(new java.io.InputStreamReader((response.getEntity().getContent())));
  String output;
  System.out.println("============Output:============");
  
  // Simply iterate through XML response and show on console.
  while ((output = br.readLine()) != null) {
    if(output.contains("\"subprocess\"")) {
      System.out.println("CUTTING == " + output.substring(output.indexOf(":") + 2));
      String subprocess = output.substring(output.indexOf(":") + 2, output.indexOf(",")-2);
      System.out.println("==> SUBPROCESS HERE:" + subprocess);
      pem.setSubprocess(subprocess);
    }
    if(output.contains("\"message\"")) {
        String message = output.substring(output.indexOf(":") + 2, output.length() - 2);
        System.out.println("==> MESSAGE HERE:" + message);
        pem.setMessage(message);
      }
    System.out.println(output);
  }
} catch (org.apache.http.client.ClientProtocolException e) {
  e.printStackTrace();
} catch (java.io.IOException e) {
  e.printStackTrace();
}]]></drools:script>
